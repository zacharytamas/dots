{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode@2.7.2",
    "opencode-openai-codex-auth@4.2.0",
    // "@tarquinen/opencode-dcp@latest",
    "file:///Users/zacharyjones/Development/github.com/zacharytamas/silo"
  ],
  "experimental": {},
  "instructions": ["AGENTS.md"],
  "tui": {
    "scroll_acceleration": { "enabled": true }
  },
  "tools": { "beads": false },
  "model": "anthropic/claude-opus-4-5",
  "small_model": "anthropic/claude-haiku-4-5",
  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "enabled": true
    },
    "fetch": {
      "type": "local",
      "command": ["uvx", "mcp-server-fetch"]
    },
    // "morph": {
    //   "type": "local",
    //   "command": ["bunx", "@morphllm/morphmcp"],
    //   "environment": {
    //     "MORPH_API_KEY": "{env:MORPH_API_KEY}",
    //     "ENABLED_TOOLS": "warpgrep_codebase_search"
    //   }
    // }
  },
  "formatter": {
    "ruff": {
      "command": ["uvx", "ruff", "format", "$FILE"],
      "extensions": [".py", ".pyx", ".pyi", ".pyw", ".pyz"]
    },
    "biome": {
      "command": ["bunx", "biome", "format", "--write", "$FILE"],
      "extensions": [".js", ".jsx", ".ts", ".tsx", ".json", ".jsonc"]
    },
    "prettier": {
      "command": ["bunx", "prettier", "--write", "$FILE"],
      "extensions": [".md", ".yaml", ".yml", ".css", ".scss"]
    }
  },
  "provider": {
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": ["reasoning.encrypted_content"],
        "store": false
      },
      "models": {
